#include<stdio.h>
#include<string.h>
int main()
{
    char string[1000];
    int i=0;
    printf("\nEnter String");
    fgets(string,sizeof(string),stdin);
    
    while(string[i]!='\0')
    {
        printf("%c",string[i]);
        if(string[i]==' ')
        {
            printf("\n");
        }
        i++;
    }
    
    //printf("\n");
    //printf("%s",string);
}
